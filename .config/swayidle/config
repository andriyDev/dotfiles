timeout 600 'loginctl lock-session $XDG_SESSION_ID'
timeout 660 'swaymsg "output * dpms off"' resume 'swaymsg "output * dpms on"'
before-sleep 'loginctl lock-session $XDG_SESSION_ID'
lock 'dunstctl set-paused true; (swaylock; loginctl unlock-session $XDG_SESSION_ID) &'
unlock 'dunstctl set-paused false'
